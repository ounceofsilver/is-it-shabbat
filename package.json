{
  "name": "is-it-shabbat-app",
  "version": "2.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "author": "James Fulford <james.fulford@outlook.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/ounceofsilver/is-it-shabbat"
  },
  "homepage": "https://isitshabbat.net",
  "scripts": {
    "start": "expo start",
    "test": "jest",
    "ci": "yarn && yarn test --ci --coverage",
    "tdd": "yarn test --watch",
    "web:start": "expo start --web",
    "web:build": "./build.sh",
    "web:local:serve": "lite-server --baseDir='web-build'",
    "web:local": "yarn web:build && yarn web:local:serve",
    "web:e2e:open": "cypress open",
    "web:e2e:ci": "cypress run",
    "web:e2e": "start-server-and-test web:start 19006 web:e2e:ci",
    "web:deploy": "yarn ci && yarn web:e2e && yarn web:build && gh-pages -d web-build",
    "app:deploy": "yarn ci && expo publish"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn ci --only-changed",
      "pre-push": "yarn ci"
    }
  },
  "jest": {
    "presets": [
      "@babel/preset-react",
      "jest-expo"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "./src/**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)+(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@expo/webpack-config": "^0.17.4",
    "@react-navigation/native": "6.1.2",
    "@react-navigation/native-stack": "6.9.8",
    "@react-navigation/stack": "6.3.11",
    "axios": "0.19.0",
    "expo": "~47.0.13",
    "expo-asset": "~8.7.0",
    "expo-font": "~11.0.1",
    "expo-keep-awake": "~11.0.1",
    "expo-localization": "~14.0.0",
    "expo-location": "~15.0.1",
    "expo-status-bar": "~1.4.2",
    "i18n-js": "3.2.2",
    "lodash.debounce": "4.0.8",
    "luxon": "1.8.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-google-maps": "9.4.5",
    "react-native": "0.70.5",
    "react-native-gesture-handler": "~2.8.0",
    "react-native-maps": "1.3.2",
    "react-native-safe-area-context": "4.4.1",
    "react-native-screens": "~3.18.0",
    "react-native-web": "~0.18.9",
    "react-navigation": "4.4.4",
    "react-redux": "8.0.5",
    "reactn": "2.2.7",
    "redux": "4.2.1",
    "redux-thunk": "2.4.2",
    "shabbat-logic": "1.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@babel/preset-env": "7.20.2",
    "@types/i18n-js": "3.8.4",
    "@types/jest": "29.4.0",
    "@types/react": "18.0.27",
    "@types/react-native": "0.70.9",
    "@types/react-test-renderer": "18.0.0",
    "babel-jest": "29.4.1",
    "cypress": "3.4.1",
    "gh-pages": "5.0.0",
    "husky": "8.0.3",
    "jest": "29.4.1",
    "jest-expo": "47.0.1",
    "lite-server": "2.5.4",
    "react-test-renderer": "16.5.0",
    "sonarqube-scanner": "2.4.1",
    "start-server-and-test": "1.10.6",
    "ts-jest": "29.0.5",
    "typescript": "4.9.4"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts"
    ]
  }
}
