{
  "name": "is-it-shabbat-app",
  "version": "1.0.1",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "author": "James Fulford <james.fulford@outlook.com>",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "lint": "eslint ./src/**/*.js",
    "test": "mocha ./src/**/*.spec.js",
    "report": "nyc --reporter=lcov --reporter=text npm run test",
    "scan": "npm run report && bash scripts/scan.sh",
    "tdd": "npm run test -- -w",
    "dev:rebuild": "bash scripts/rebuild-core.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    }
  },
  "nyc": {
    "include": "src",
    "exclude": "**/*.spec.js"
  },
  "dependencies": {
    "@expo/vector-icons": "8.0.0",
    "expo": "31.0.2",
    "i18next": "13.0.0",
    "is-it-shabbat-core": "1.0.3",
    "luxon": "1.8.0",
    "prop-types": "15.6.2",
    "react": "16.5.0",
    "react-i18next": "8.4.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-31.0.0.tar.gz",
    "react-navigation": "3.0.0",
    "react-redux": "6.0.0",
    "styled-components": "4.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/polyfill": "7.2.5",
    "@babel/register": "7.0.0",
    "babel-eslint": "10.0.1",
    "babel-preset-expo": "5.0.0",
    "chai": "4.2.0",
    "chai-enzyme": "1.0.0-beta.1",
    "dirty-chai": "2.0.1",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "eslint": "5.9.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "expo-cli": "2.6.14",
    "husky": "1.2.1",
    "jsdom": "13.1.0",
    "mocha": "5.2.0",
    "mocha-eslint": "4.1.0",
    "nyc": "13.1.0",
    "proxyquire": "2.1.0",
    "react-dom": "16.7.0",
    "sinon": "7.1.1",
    "sinon-chai": "3.2.0",
    "sonarqube-scanner": "2.1.2"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts"
    ]
  }
}
